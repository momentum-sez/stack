{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.momentum-sez.org/msez/corridor.receipt.schema.json",
  "title": "MSEZ Corridor State Receipt",
  "type": "object",
  "required": [
    "type",
    "corridor_id",
    "sequence",
    "timestamp",
    "prev_root",
    "next_root",
    "lawpack_digest_set",
    "ruleset_digest_set",
    "proof"
  ],
  "properties": {
    "type": {
      "const": "MSEZCorridorStateReceipt",
      "description": "Receipt kind identifier."
    },
    "corridor_id": {
      "type": "string",
      "minLength": 1
    },
    "sequence": {
      "type": "integer",
      "minimum": 0,
      "description": "Monotonic sequence number within a corridor state channel."
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "RFC3339 timestamp (UTC recommended)."
    },
    "prev_root": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$",
      "description": "State root before this transition. For the first receipt this SHOULD equal the corridor genesis_root."
    },
    "next_root": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$",
      "description": "State root after this transition. MUST equal SHA256(JCS(receipt_without_proof_and_next_root))."
    },
    "lawpack_digest_set": {
      "type": "array",
      "items": {
        "description": "A raw sha256 digest string (legacy) OR an ArtifactRef (recommended; artifact_type MUST be 'lawpack').",
        "oneOf": [
          {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          {
            "allOf": [
              {
                "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
              },
              {
                "properties": {
                  "artifact_type": {
                    "const": "lawpack"
                  }
                },
                "required": [
                  "artifact_type",
                  "digest_sha256"
                ]
              }
            ]
          }
        ]
      },
      "description": "Sorted set of lawpack digests (sha256) governing this corridor operation snapshot."
    },
    "ruleset_digest_set": {
      "type": "array",
      "items": {
        "description": "A raw sha256 digest string (legacy) OR an ArtifactRef (recommended; artifact_type MUST be 'ruleset').",
        "oneOf": [
          {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          {
            "allOf": [
              {
                "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
              },
              {
                "properties": {
                  "artifact_type": {
                    "const": "ruleset"
                  }
                },
                "required": [
                  "artifact_type",
                  "digest_sha256"
                ]
              }
            ]
          }
        ]
      },
      "description": "Sorted set of ruleset digests (sha256) governing this corridor transition."
    },
    "transition_type_registry_digest_sha256": {
      "description": "Optional digest commitment to a Transition Type Registry snapshot (snapshot_digest_sha256 from transition-types.lock.json). When present, receipts MAY omit per-transition schema/ruleset/circuit digests and rely on the registry mapping for semantics. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='transition-types'.",
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        {
          "allOf": [
            {
              "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
            },
            {
              "properties": {
                "artifact_type": {
                  "const": "transition-types"
                }
              },
              "required": [
                "artifact_type",
                "digest_sha256"
              ]
            }
          ]
        }
      ]
    },
    "transition": {
      "description": "Corridor transition envelope. v0.4.3+ RECOMMENDS typed envelopes; legacy objects are accepted for backward compatibility.",
      "oneOf": [
        {
          "type": "object",
          "required": [
            "type",
            "kind",
            "payload_sha256"
          ],
          "properties": {
            "type": {
              "const": "MSEZTransitionEnvelope",
              "description": "Typed envelope marker."
            },
            "kind": {
              "type": "string",
              "minLength": 1,
              "description": "Corridor-specific transition kind (e.g., 'mint', 'burn', 'settle', 'attest')."
            },
            "schema": {
              "type": "string",
              "description": "Optional schema identifier/URI for the payload."
            },
            "schema_digest_sha256": {
              "description": "Optional payload schema commitment. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='schema'.",
              "oneOf": [
                {
                  "type": "string",
                  "pattern": "^[a-f0-9]{64}$"
                },
                {
                  "allOf": [
                    {
                      "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                    },
                    {
                      "properties": {
                        "artifact_type": {
                          "const": "schema"
                        }
                      },
                      "required": [
                        "artifact_type",
                        "digest_sha256"
                      ]
                    }
                  ]
                }
              ]
            },
            "ruleset_digest_sha256": {
              "description": "Optional transition ruleset commitment. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='ruleset'.",
              "oneOf": [
                {
                  "type": "string",
                  "pattern": "^[a-f0-9]{64}$"
                },
                {
                  "allOf": [
                    {
                      "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                    },
                    {
                      "properties": {
                        "artifact_type": {
                          "const": "ruleset"
                        }
                      },
                      "required": [
                        "artifact_type",
                        "digest_sha256"
                      ]
                    }
                  ]
                }
              ]
            },
            "zk_circuit_digest_sha256": {
              "description": "Optional ZK circuit commitment for proof-carrying transitions. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='circuit'.",
              "oneOf": [
                {
                  "type": "string",
                  "pattern": "^[a-f0-9]{64}$"
                },
                {
                  "allOf": [
                    {
                      "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                    },
                    {
                      "properties": {
                        "artifact_type": {
                          "const": "circuit"
                        }
                      },
                      "required": [
                        "artifact_type",
                        "digest_sha256"
                      ]
                    }
                  ]
                }
              ]
            },
            "registry_override": {
              "type": "boolean",
              "description": "Optional escape hatch. When true, any digest references present in this envelope are treated as explicit overrides of the registry mapping for this receipt."
            },
            "payload": {
              "description": "Optional inline payload (may be omitted for privacy; payload_sha256 still commits).",
              "oneOf": [
                {
                  "type": "object"
                },
                {
                  "type": "array"
                },
                {
                  "type": "string"
                },
                {
                  "type": "number"
                },
                {
                  "type": "boolean"
                },
                {
                  "type": "null"
                }
              ]
            },
            "payload_sha256": {
              "type": "string",
              "pattern": "^[a-f0-9]{64}$",
              "description": "SHA256(JCS(payload)) when payload is present, otherwise SHA256 of an out-of-band payload per corridor policy."
            },
            "attachments": {
              "type": "array",
              "description": "Optional list of referenced artifacts committed by digest. Attachments are typed artifact references so they can point at blobs, schemas, VCs, checkpoints, etc.",
              "items": {
                "oneOf": [
                  {
                    "type": "object",
                    "description": "Legacy attachment reference (v0.4.9 and earlier): untyped digest treated as a `blob` artifact.",
                    "required": [
                      "uri",
                      "digest_sha256"
                    ],
                    "properties": {
                      "uri": {
                        "type": "string"
                      },
                      "digest_sha256": {
                        "type": "string",
                        "pattern": "^[a-f0-9]{64}$",
                        "description": "SHA256 over raw attachment bytes. Resolves as a `blob` artifact: dist/artifacts/blob/<digest>.*"
                      },
                      "media_type": {
                        "type": "string"
                      },
                      "byte_length": {
                        "type": "integer",
                        "minimum": 0
                      }
                    },
                    "additionalProperties": true,
                    "not": {
                      "required": [
                        "artifact_type"
                      ]
                    }
                  },
                  {
                    "allOf": [
                      {
                        "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                      }
                    ],
                    "description": "Typed artifact attachment reference (v0.4.10+)."
                  }
                ]
              }
            },
            "meta": {
              "type": "object",
              "description": "Optional free-form metadata (timestamps, operator ids, counters).",
              "additionalProperties": true
            }
          },
          "additionalProperties": true
        },
        {
          "type": "object",
          "description": "Legacy untyped transition object (accepted for backward compatibility).",
          "not": {
            "properties": {
              "type": {
                "const": "MSEZTransitionEnvelope"
              }
            }
          },
          "additionalProperties": true
        }
      ]
    },
    "zk": {
      "type": "object",
      "description": "Optional ZK proof scaffold. Proof verification is corridor-specific; receipts commit to proof and verifier digests.",
      "properties": {
        "proof_system": {
          "type": "string"
        },
        "circuit_digest_sha256": {
          "description": "Optional commitment to the circuit artifact. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='circuit'.",
          "oneOf": [
            {
              "type": "string",
              "pattern": "^[a-f0-9]{64}$"
            },
            {
              "allOf": [
                {
                  "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                },
                {
                  "properties": {
                    "artifact_type": {
                      "const": "circuit"
                    }
                  },
                  "required": [
                    "artifact_type",
                    "digest_sha256"
                  ]
                }
              ]
            }
          ]
        },
        "verifier_key_digest_sha256": {
          "description": "Optional commitment to the verifier/proving key artifact. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='proof-key'.",
          "oneOf": [
            {
              "type": "string",
              "pattern": "^[a-f0-9]{64}$"
            },
            {
              "allOf": [
                {
                  "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                },
                {
                  "properties": {
                    "artifact_type": {
                      "const": "proof-key"
                    }
                  },
                  "required": [
                    "artifact_type",
                    "digest_sha256"
                  ]
                }
              ]
            }
          ]
        },
        "public_inputs": {
          "type": "object",
          "additionalProperties": true
        },
        "proof_sha256": {
          "description": "Commitment to raw proof bytes. Legacy form is a raw sha256 digest; preferred form is an ArtifactRef with artifact_type='blob'. Resolves as: dist/artifacts/blob/<digest>.*",
          "oneOf": [
            {
              "type": "string",
              "pattern": "^[a-f0-9]{64}$"
            },
            {
              "allOf": [
                {
                  "$ref": "https://schemas.momentum-sez.org/msez/artifact-ref.schema.json"
                },
                {
                  "properties": {
                    "artifact_type": {
                      "const": "blob"
                    }
                  },
                  "required": [
                    "artifact_type",
                    "digest_sha256"
                  ]
                }
              ]
            }
          ]
        },
        "proof": {
          "type": "string",
          "description": "Optional proof bytes (base64 or other encoding, corridor-specific)."
        },
        "proof_uri": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "anchor": {
      "type": "object",
      "description": "Optional anchoring metadata for external chain commitments.",
      "properties": {
        "chain_id": {
          "type": "string"
        },
        "method": {
          "type": "string"
        },
        "tx_hash": {
          "type": "string"
        },
        "block_number": {
          "type": "integer"
        },
        "anchored_root": {
          "type": "string",
          "pattern": "^[a-f0-9]{64}$"
        },
        "anchored_at": {
          "type": "string",
          "format": "date-time"
        }
      },
      "additionalProperties": true
    },
    "proof": {
      "description": "One or more cryptographic proofs over the receipt payload (excluding the proof field).",
      "oneOf": [
        {
          "type": "object",
          "required": [
            "type",
            "created",
            "verificationMethod",
            "proofPurpose",
            "jws"
          ],
          "properties": {
            "type": {
              "const": "MsezEd25519Signature2025"
            },
            "created": {
              "type": "string",
              "format": "date-time"
            },
            "verificationMethod": {
              "type": "string",
              "minLength": 1
            },
            "proofPurpose": {
              "type": "string",
              "minLength": 1
            },
            "jws": {
              "type": "string",
              "minLength": 1
            }
          },
          "additionalProperties": false
        },
        {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": [
              "type",
              "created",
              "verificationMethod",
              "proofPurpose",
              "jws"
            ],
            "properties": {
              "type": {
                "const": "MsezEd25519Signature2025"
              },
              "created": {
                "type": "string",
                "format": "date-time"
              },
              "verificationMethod": {
                "type": "string",
                "minLength": 1
              },
              "proofPurpose": {
                "type": "string",
                "minLength": 1
              },
              "jws": {
                "type": "string",
                "minLength": 1
              }
            },
            "additionalProperties": false
          }
        }
      ]
    }
  },
  "additionalProperties": true
}
