{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Trust Company Incident Report",
  "type": "object",
  "required": [
    "incident_id",
    "reported_at",
    "license_id",
    "category",
    "severity",
    "summary"
  ],
  "properties": {
    "incident_id": {
      "type": "string"
    },
    "reported_at": {
      "type": "string",
      "format": "date-time"
    },
    "license_id": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "severity": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    }
  }
}